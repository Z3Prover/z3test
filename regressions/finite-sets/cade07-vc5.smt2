(set-logic QF_UFLIAFS)
(declare-fun content!16 () (Set Int))
(declare-fun alloc0!17 () (Set Int))
(declare-fun x1!18 () Int)
(declare-fun alloc1!19 () (Set Int))
(declare-fun x2!20 () Int)
(declare-fun alloc2!21 () (Set Int))
(declare-fun x3!22 () Int)
(assert
(and (subset content!16 alloc0!17)
     (not (member x1!18 alloc0!17))
     (subset (union alloc0!17 (singleton x1!18)) alloc1!19)
     (not (member x2!20 alloc1!19))
     (subset (union alloc1!19 (singleton x2!20)) alloc2!21)
     (not (member x3!22 alloc2!21))
     (distinct (card (union content!16
                                   (union (union (singleton x1!18)
                                                 (singleton x2!20))
                                          (singleton x3!22))))
               (+ (card content!16) 3)))
)
(check-sat)
